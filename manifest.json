{
	"name": "Virtual Meeting Leaver",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"description": "Leave Google Meet and BigBlueButton conferences at a specified times.",
	"icons": {
		"128": "icon.png"
	},
	"background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
	"browser_action": {
		"default_popup": "popup.html"
	},
	
	"permissions": [
    "storage",
	"tabs"
  ]
}
